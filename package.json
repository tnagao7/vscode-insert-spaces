{
    "name": "insertspace",
    "displayName": "Insert Spaces between Half/Fullwidths",
    "description": "Inserting spaces between full- and half-width characters.",
    "version": "1.0.0",
    "publisher": "tnagao",
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.insertspace"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.insertspace",
                "title": "Insert Spaces between Full/Halfwidth Characters"
            }
        ],
        "keybindings": [
            {
                "command": "extension.insertspace",
                "key": "Ctrl+Shift+J"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
