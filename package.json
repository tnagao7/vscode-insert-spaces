{
    "name": "insert-spaces-half-full",
    "displayName": "Insert Spaces between Half/Fullwidths",
    "description": "Inserts spaces between full- and half-width characters.",
    "version": "1.1.0",
    "publisher": "wgag",
    "repository" : "https://github.com/wgag/vscode-insert-spaces",
    "engines": {
        "vscode": "^1.11.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.insertspace"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.insertspace",
                "title": "Insert Spaces between Full/Halfwidths"
            }
        ],
        "keybindings": [
            {
                "command": "extension.insertspace",
                "key": "Ctrl+Shift+I"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.4.2",
        "vscode": "^1.1.4",
        "mocha": "^2.5.0",
        "@types/node": "^8.0.20",
        "@types/mocha": "^2.2.41"
    }
}
